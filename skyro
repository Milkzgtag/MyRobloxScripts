local function XNWKO_fake_script() -- loginbutton.LocalScript 
	local script = Instance.new('LocalScript', loginbutton)

	script.Parent.MouseButton1Click:Connect(function()
	local player = game.Players.LocalPlayer
	local user = game.Players.LocalPlayer.PlayerGui.skyrologin.login.Username.Text
	local pass = game.Players.LocalPlayer.PlayerGui.skyrologin.login.Password.Text
	
		if user == "shiba" and pass == "shiba is sussy" then
			game.Players.LocalPlayer.PlayerGui.skyrologin.login.loginbutton.Text = "Login Successful!"
			wait(0.4)
			player.PlayerGui.skyrologin:Destroy()
			loadstring(game:HttpGet('https://raw.githubusercontent.com/ShibaGT/ScriptsToPowerOtherScripts/main/Loadstring%20Support'))()
		else
			game.Players.LocalPlayer.PlayerGui.skyrologin.login.loginbutton.Text = "Invalid Login!"
			wait(0.4)
			game.Players.LocalPlayer.PlayerGui.skyrologin.login.loginbutton.Text = "Login"
		end
		
		if user == "DogeyGt" and pass == "Dogeyballs" then
			game.Players.LocalPlayer.PlayerGui.skyrologin.login.loginbutton.Text = "Login Successful!"
			wait(0.4)
			player.PlayerGui.skyrologin:Destroy()
			loadstring(game:HttpGet('https://raw.githubusercontent.com/ShibaGT/ScriptsToPowerOtherScripts/main/Loadstring%20Support'))()
		else
			game.Players.LocalPlayer.PlayerGui.skyrologin.login.loginbutton.Text = "Invalid Login!"
			wait(0.4)
			game.Players.LocalPlayer.PlayerGui.skyrologin.login.loginbutton.Text = "Login"
		end
		
		if user == "WATER" and pass == "BOTTLE" then
			game.Players.LocalPlayer.PlayerGui.skyrologin.login.loginbutton.Text = "Login Successful!"
			wait(0.4)
			player.PlayerGui.skyrologin:Destroy()
			loadstring(game:HttpGet('https://raw.githubusercontent.com/ShibaGT/ScriptsToPowerOtherScripts/main/Loadstring%20Support'))()
		else
			game.Players.LocalPlayer.PlayerGui.skyrologin.login.loginbutton.Text = "Invalid Login!"
			wait(0.4)
			game.Players.LocalPlayer.PlayerGui.skyrologin.login.loginbutton.Text = "Login"
		end
	end)
end
coroutine.wrap(XNWKO_fake_script)()
local function LBAAJ_fake_script() -- login.Drag 
	local script = Instance.new('LocalScript', login)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(LBAAJ_fake_script)()
