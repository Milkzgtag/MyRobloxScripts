-- Scripts:

local function EVBASRJ_fake_script() -- login.colorchange 
	local script = Instance.new('LocalScript', login)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	
	counter = 0
	
	while wait(0.01)do
		script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
	
		counter = counter + 0.01
	end
end
coroutine.wrap(EVBASRJ_fake_script)()
local function FZXBYUE_fake_script() -- Username.colorchange 
	local script = Instance.new('LocalScript', Username)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	
	counter = 0
	
	while wait(0.01)do
		script.Parent.PlaceholderColor3 = Color3.fromHSV(zigzag(counter),1,1)
	
		counter = counter + 0.01
	end
end
coroutine.wrap(FZXBYUE_fake_script)()
