-- Scripts:

local function OWDIJI_fake_script() -- Frame.Drag 
	local script = Instance.new('LocalScript', Frame)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(OWDIJI_fake_script)()
local function JKVJOS_fake_script() -- Frame.Script 
	local script = Instance.new('Script', Frame)

	local player = script:FindFirstAncestorOfClass("Player")
	local frame = script.Parent
	local Remote = script.Parent.GearIdRemote
	local scrollingframe = script.Parent.ScrollingFrame
	local gears = script.Parent.Gear
	
	for i, v in pairs(gears:GetChildren()) do
		if v and v:IsA("Tool") then
			local example = script.Example:Clone()
			example.Name = v.Name
			example.Text = v.Name
			example.Gear.Value = v.Name
			example.MouseButton1Down:Connect(function()
				if player then
					local gear = gears:FindFirstChild(example.Gear.Value)
					if gear then
						local gearclone = gear:Clone()
						gearclone.Parent = player.Backpack
					end
				end
			end)
			example.Parent = scrollingframe
		end
	end
	
	Remote.OnServerEvent:Connect(function(Client, Id)
		if player == Client then
			if tonumber(Id) then
				game:GetService("InsertService"):LoadAsset(tonumber(Id)):GetChildren()[1].Parent=player.Backpack
			end
		end
	end)
	
	script.Parent.Super.MouseButton1Down:Connect(function()
		script.Parent.Gear.ModernRocketLauncher:Clone().Parent = player.Backpack
	end)
end
coroutine.wrap(JKVJOS_fake_script)()
local function PUOMZO_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local Remote = script.Parent.GearIdRemote
	script.Parent.TextButton.MouseButton1Down:Connect(function()
		Remote:FireServer(script.Parent.TextBox.Text)
	end)
end
coroutine.wrap(PUOMZO_fake_script)()
local function VLVY_fake_script() -- Exit.LocalScript 
	local script = Instance.new('LocalScript', Exit)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(VLVY_fake_script)()
